<?xml version="1.0" encoding="UTF-8"?>
<Context antiJARLocking="true" path="/3.4.3ServletJPA">
    <Resource
    name="jdbc/webdb"
    auth="Container"
    type="javax.sql.DataSource"
    maxActive="100"
    maxIdle="5"
    driverClassName="org.postgresql.Driver"
    url="jdbc:postgresql://localhost:5432/webdb"
    username="webuser"
    password="webuser"
    />
</Context>
  
